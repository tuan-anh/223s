set bwid 6
set xmgr 1				* X MarGin Right
set ymgu 1				* Y MarGin Up
set xmgl 1.				* X MarGin Left
set ymgl 2.5				* Y MarGin Low
set hwid 5.
set vsize .15
zon 5 5
set xwin 0.8					* distance between windows
set ywin 0.005
opt logy
do jj=1,5
do ii=1,5
null 0. 38. 2. 500.
id1=5*(5-[jj])+[ii]+400
id2=5*(5-[jj])+[ii]+425
set hcol 2
h/pl [id1] s
set hcol 4
h/pl [id2] s
enddo
enddo